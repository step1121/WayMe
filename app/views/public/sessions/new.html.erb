<%= form_with model: @user, url: session_path(resource_name), local: true do |f| %>
  <div class="field">
    <%= f.label :name, "名前" %><br />
    <%= f.text_field :name, autofocus: true, autocomplete: "neme" %>
  </div>

  <div class="field">
    <%= f.label :password, "パスワード" %></div>
    <%= f.password_field :password, autocomplete: "user-password" %></div>
  </div>

  <div class="actions">
    <%= f.submit "ログイン" %>
  </div>
<% end %>

<%= render "public/shared/links" %>
