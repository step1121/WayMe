<% if user_signed_in? %>
    <%= form_with url: search_path do |f|  %>
      <%= f.text_field 'content' %>
      <%= f.select 'model', options_for_select({ "User" => "user", "Vision" => "vision" }) %>
      <button class="btn btn-secondary opacity-50 rounded ml-2"><i class="fas fa-search"></i></button>
    <% end %>
<% end %>